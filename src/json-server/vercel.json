{
    "version": 2,
    "builds": [
      {
        "src": "/src/json-server/server.js",
        "use": "@vercel/node",
        "config": {
          "includeFiles": ["db.json"]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/src/json-server/server.js"
      }
    ]
  }